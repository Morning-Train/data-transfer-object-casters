{
    "name": "morningtrain/data-transfer-object-casters",
    "description": "A package containing Data Transfer Object casters, for use with spatie/data-transfer-object",
    "keywords": [
        "morningtrain",
        "laravel",
        "data-transfer-object-casters"
    ],
    "homepage": "https://github.com/morningtrain/data-transfer-object-casters",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon JÃ¸nsson",
            "email": "sj@morningtrain.dk"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "spatie/data-transfer-object": "^3.7",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Morningtrain\\DataTransferObjectCasters\\": "src",
            "Morningtrain\\DataTransferObjectCasters\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morningtrain\\DataTransferObjectCasters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
